                    font_size: 20
                    bold: True
                    background_color:(0,1,1,1)
                    color: (0,0,0,1)
                    size_hint:0.3,0.05
                    valign: "middle"
                    canvas.before:
                        Color:
                            rgba: (0,0,0,1)
                        Line:
                            width: 1.1
