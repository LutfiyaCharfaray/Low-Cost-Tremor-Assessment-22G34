<SaveScreen>:
    name: "end"
    FloatLayout:
        canvas.before:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "blue.jpeg"

        Button:
            text: "Return to Menu"
            font_size: "30"
            size_hint: 0.3, 0.2
            pos_hint: {'x': .1, 'y': 0.3}
            background_color: (0,1,1,1)
            on_press: root.change_dir()
            on_release:
                app.root.current = "menu"
                root.manager.transition.direction = "right"

        Button:
            text: "Return to Login"
            font_size: "30"
            size_hint: 0.3, 0.2
            pos_hint: {'x': .6, 'y': .3} 
            background_color: (0,1,1,1)
            on_press: root.change_dir()
            on_release:
                app.root.current = "login"
                root.manager.transition.direction = "right"
