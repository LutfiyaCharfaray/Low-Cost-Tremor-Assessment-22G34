WindowManager: # define screens
    LoginWindow:
    MenuWindow:
    RegWindow:
    SearchWindow:
    StartScreen:
    PracScreen:
    DSpiralScreen:
    VRScreen1:
        id: slider1
        name: 'rating1'
        manager: 'screen_manager'
    NdSpiralScreen:
    VRScreen2:
        id: slider2
        name: 'rating2'
        manager: 'screen_manager'
    TickBox:
    ResultScreen1:
        id: result1
        name: 'results_1'
        manager: 'screen_manager'
        vrs1: slider1.vrs1
        vrs2: slider2.vrs2
    ResultScreen2:
    SaveScreen:
           
<LoginWindow>:
    name: "login"

    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: "blue.jpeg" 

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        padding: 50
        spacing: 40

        Label:
            text: "Login with Password"
            bold: True
            pos_hint: {'center_x': 0.5} 
            size_hint: (None, None)
            width: 400
            color: (0,0,0,1) 
            font_size: 32

        TextInput:
            id: pw
            multiline: False
            hint_text: 'Please Enter Password:'
            pos_hint: {'center_x': 0.5} 
            size_hint: (None, None)
            width: 500
            height: 80
            password: True
            padding: [30,30,20,20]

        Button:
            text: "Submit"
            pos_hint: {'center_x': 0.5}
            size_hint: (None, None)
            width: 200
            height: 50
            background_color: (0,1,1,1) 
            color: (1,1,1,1) 
            on_release: 
                app.root.current = "menu" if pw.text == "tremordr" else "login"
                root.manager.transition.direction = "left"
                pw.text = "" #to clear textbox input

<MenuWindow>:
    name: "menu"
    FloatLayout:
        canvas.before:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "blue.jpeg"
        FloatLayout: 

            Button:
                background_color: 0,1,1,1
                text: "New Patient"
                font_size: "30dp"
                size_hint: 0.3, 0.2
                pos_hint: {'x': .1, 'y': 0.3}
                on_release:
                    app.root.current="form"
                    root.manager.transition.direction = "left"

            Button:
                background_color: 0,1,1,1
                text: "Existing Patient"
                font_size: "30dp"
                size_hint: 0.3, 0.2
                pos_hint: {'x': .6, 'y': .3} 
                on_release:
                    app.root.current="search"
                    root.manager.transition.direction = "left"

<RegWindow>:
    name: "form"

    ptname: ptname
    ptsname: ptsname
    ptage: ptage
    ptnum: ptnum
    
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: "blue.jpeg"

    FloatLayout:

        Label:
            text: "Please Enter Patient Details"
            font_size: 20
            bold: True
            color: (0,0,0,1) 
            pos_hint: {'right': 0.7, 'y':0.45} 

        BoxLayout:
            orientation: 'vertical'
            size: root.width, root.height

            padding: 20
            spacing: 20 

            GridLayout:
                cols: 2
                padding: [20,20,20,20]
                spacing: 20

                Label:
                    text: "Name: "
                    color: (0,0,0,1) 
                    font_size: 20
                    halign: 'left'

                TextInput:
                    id: ptname
                    multiline: False
                    hint_text: 'Enter patient name'
                    padding: 20 

                Label:
                    text: "Surname: "
                    color: (0,0,0,1) 
                    font_size: 20
                    halign: 'left'
                    
                TextInput:
                    id: ptsname
                    multiline: False
                    hint_text: 'Enter patient surname'
                    padding: 20 

                Label:
                    text: "Age: "
                    color: (0,0,0,1) 
                    font_size: 20
                    halign: 'left'

                TextInput:
                    id: ptage
                    multiline: False
                    min_value: 1
                    max_value: 100
                    hint_text: 'Enter an age between {} and {}'.format(self.min_value, self.max_value)
                    padding: 20

                Label:
                    text: "Contact Number: "
                    color: (0,0,0,1) 
                    font_size: 20
                    halign: 'left'

                TextInput:
                    id: ptnum
                    multiline: False
                    hint_text: 'Enter a 10 digit phone number'
                    padding: 20 

            Button:
                text: "Submit"
                background_color: (0,1,1,1)
                color: (1,1,1,1)
                pos_hint: {'x': 0.6}
                size_hint: (None, None)
                width: 200
                height: 50
                on_press: root.submit()

            Button:
                text: "Next"
                background_color: (0,1,1,1)
                color: (1,1,1,1)
                pos_hint: {'x': 0.6}
                size_hint: (None, None)
                width: 200
                height: 50
                on_release:
                    app.root.current = "search" 
                    root.manager.transition.direction = "left"

            Button:
                text: "Return to menu"
                pos_hint: {'left': 1, 'bottom' : 1} 
                size_hint: (None, None)
                width: 200
                height: 50
                font_size: 20
                background_color: (0,1,1,1)
                color: (1,1,1,1) 
                on_release:
                    app.root.current="menu"
                    root.manager.transition.direction = "right"
                    
<SearchWindow>:
    name: "search"
    word_input:word_input
    surname_input:surname_input
    
    FloatLayout:
        canvas.before:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "blue.jpeg"

        Label:
            text: "Please Enter Patient Name and Surname"
            font_size: 20
            bold: True
            color: (0,0,0,1) 
            pos_hint: {'center_x': 0.5, 'y':0.45} 
        
        FloatLayout:
            Label:
                id : word_label
                text: "Enter patient name :"
                pos_hint: {'x': 0.2, 'y' : 0.7}
                size_hint: (0.2, 0.1)
                color: (0,0,0,1)
                
            TextInput:
                id: word_input
                multiline: False
                pos_hint: {'x': 0.5 , 'y' : 0.7}
                size_hint: (0.3, 0.1)

            Label:
                id : surname_label
                text: "Enter patient surname :"
                pos_hint: {'x': 0.2, 'y' : 0.5}
                size_hint: (0.2, 0.1)
                color: (0,0,0,1)

            TextInput:
                id: surname_input
                multiline: False
                pos_hint: {'x': 0.5 , 'y' : 0.5}
                size_hint: (0.3, 0.1)

            Button:
                text: "Show patient details"
                pos_hint: {'x': 0.2 , 'y' : 0.3}
                size_hint: (0.2, 0.1)
                background_color: (0,1,1,1)
                on_press: root.show_id()
            
            Label:
                id : id_label
                text: "Patient ID"
                pos_hint: {'x': 0.5, 'y' : 0.3}
                size_hint: (0.3, 0.1)
                font_size: 20
                color: (0,0,0,1)

            Button:
                text: "Go to Assessment" 
                pos_hint: {'x': 0.6, 'y' : 0.05}
                size_hint: (0.3, 0.1)
                font_size: 20
                background_color: (0,1,1,1)
                on_release:
                    app.root.current="start"
                    #root.manager.transition.direction = "left"

            Button:
                text: "Return to menu"
                pos_hint: {'x': 0.1, 'y' : 0.05}
                size_hint: (0.3, 0.1)
                font_size: 20
                background_color: (0,1,1,1)
                on_release:
                    app.root.current="menu"
                    root.manager.transition.direction = "right"

<StartScreen>:
    name: "start"
    Button: 
        text: "Start Assessment"
        color: (1,1,1,1)
        background_color: (0,1,1,1)
        font_size: 40
        on_release:
            app.root.current = "prac"
            root.manager.transition.direction = "left"

<LineWidget>:
    canvas.before:
        Rectangle:
            size: self.size  
            pos: self.pos
            source: "linetemp.png"

<DrawLine>:
    Button: 
        id: 'clearbutton'
        text: "Clear"
        right: root.right
        top: root.top
        font_size: 20
        width: 80
        height:40
        color: (1,1,1,1)
        background_color: (0,1,1,1)
        on_release: root.reset_canvas()
        
<PracScreen>:
    name: "prac"

    on_pre_enter:
        draw.reset_canvas() 

    FloatLayout:
        id: export1
        LineWidget:

        Label:
            text: "Draw a line within the box without lifting your finger"
            pos_hint: {"center_x": 0.5, "top": 1.3}
            font_size: 30
            color: (1,1,1,1)

        DrawLine:
            id: draw

        Button:
            id: 'next'
            text: "Next"
            font_size: 20
            size_hint: 0.2, 0.1
            pos_hint: {"right": 1, "bottom": 1}
            background_color: (0,1,1,1)
            on_press: root.capture()
            on_release:
                app.root.current = "tick"
                root.manager.transition.direction = "left"

<TickBox>:
    name: "tick"
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            Color:
                rgb:1,1,1
    FloatLayout:

        Label:
            text: "Dominant Hand"
            font_size: 30
            bold: True
            color: (0,0,0,1)
            pos_hint: {'center_x': 0.5, 'y':0.45}
        FloatLayout:
            cols: 4   
            Label:
                text: "Left"
                font_size: 25
                color: (0,0,0,1)
                bold: True
                pos_hint: {'x': 0.1 , 'y' : 0.5}
                size_hint: (0.2, 0.1)
            CheckBox:
                on_active: root.checkbox_click1(self,self.active,"Left")
                pos_hint: {'x': 0.17 , 'y' : 0.5}
                size_hint: (0.3, 0.1)
                bold: True
                color: (0,0,0,1)
                width: 0.9
                font_size: 30 
            Label:
                text: "Right"
                font_size: 25
                bold: True
                color: (0,0,0,1)
                pos_hint: {'x': 0.6 , 'y' : 0.5}
                size_hint: (0.2, 0.1)
            CheckBox:
                on_active: root.checkbox_click2(self,self.active,"Right")
                pos_hint: {'x': 0.67 , 'y' : 0.5}
                size_hint: (0.3, 0.1)
                bold: True
                color: (0,0,0,1)
                width: 0.9
                font_size: 30
        Button:
            text: "Next"
            pos_hint: {'x': 0.75 , 'y' : 0.03}
            size_hint: (0.2, 0.1)
            background_color: (0,1,1,1)
            on_release:
                app.root.current = "dom_spiral"
                root.manager.transition.direction = "left"

<SpiralWidget>:
    canvas.before:
        Rectangle:
            size: self.size  
            pos: self.pos
            source: "spiraltemp.png"

<DSpiralScreen>:
    name: "dom_spiral" #dominant hand spiral

    on_pre_enter:
        draw.reset_canvas() 

    FloatLayout:
        id: export2
        SpiralWidget:

        Label:
            text: "Trace the spiral using your dominant hand."
            pos_hint: {"center_x": 0.5, "top": 1.45}
            font_size: 30
            color: (0,0,0,1)

        Label:
            text: "Start from the outside of the spiral."
            pos_hint: {"center_x": 0.5, "top": 1.36}
            font_size: 30
            color: (0,0,0,1)

        DrawLine:
            id: draw

        Button:
            id: 'next'
            text: "Next"
            font_size: 20
            size_hint: 0.2, 0.1
            pos_hint: {"right": 1, "bottom": 1}
            background_color: (0,1,1,1)
            on_press: root.capture()
            on_release:
                app.root.current = "rating1"
                root.manager.transition.direction = "left"
            
        Label:
            id : lb
            text: ''
            color: 0,0,0,1
            font_size:30
            pos_hint: {'x': 0.1 , 'y' : 0.8}
            size_hint: (0.2, 0.1)

        Button:
            text: "Stop"
            font_size: 20
            pos_hint: {'x': 0 , 'y' : 0}
            size_hint: (0.2, 0.1)
            background_color: (1,0,0,1)
            on_press: root.stop()

        Button:
            text: "Start"
            font_size: 20
            pos_hint: {'x': 0 , 'y' : 0.1}
            size_hint: (0.2, 0.1)
            background_color: (0,1,0,1)
            on_press: root.start()

<VRScreen1>:
    name: "rating1"
    vrs1: slider_.value

    on_pre_enter:
        slider_.value = 0

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height  

        Label:
            text: "Rate the Tremor from 0-10 (minor to severe) \n- dominant hand"
            font_size: 34

        Label:
            text: str(int(slider_.value))
            font_size: 30

        Slider:
            id: slider_
            min: 0
            max: 10
            step: 1
            orientation: "horizontal"
           
        Button:
            text: "Next"
            font_size: 20
            size_hint: 0.2, 0.4
            pos_hint: {"right": 1, "bottom": 1}
            background_color: (0,1,1,0.5)
            on_release:
                app.root.current = "nondom_spiral"

<NdSpiralScreen>:
    name: "nondom_spiral" # non-dominant hand spiral 

