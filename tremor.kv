                pos_hint: {"x":0.05, "y": 0.5}
                Label:
                    text: "Parameter"
                    bold:True
                    font_size: 20
                    background_color:(0,1,1,1)
                    color: (0,0,0,1)
                    size_hint:0.2,0.05
                    valign: "middle"
                    canvas.before:
                        Color:
                            rgba: (0,0,0,1)
