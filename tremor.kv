                            width: 1.1
                            rectangle: self.x, self.y, self.width, self.height      

                Label:
                    text: "Completion time(s)"
                    font_size: 16
                    background_color:(0,1,1,1)
                    color: (0,0,0,1)
                    size_hint:0.3,0.1
                    valign: "middle"
                    canvas.before:
                        Color:
                            rgba: (0,0,0,1)
                        Line:
                            width: 1.1
                            rectangle: self.x, self.y, self.width, self.height
                Label:
                    text: "14-16"
                    font_size: 16
                    background_color:(0,1,1,1)
                    color: (0,0,0,1)
                    size_hint:0.3,0.1
                    valign: "middle"
                    canvas.before:
                        Color:
                            rgba: (0,0,0,1)
                        Line:
                            width: 1.1
                            rectangle: self.x, self.y, self.width, self.height
                Label:
                    id: dom_time_label
                    text: ""
