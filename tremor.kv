            color: (0,0,0,1)

        DrawLine:
            id: draw

        Button:
            id: 'next'
            text: "Next"
            font_size: 20
            size_hint: 0.2, 0.1
            pos_hint: {"right": 1, "bottom": 1}
            background_color: (0,1,1,1)
            on_press: root.capture()
            on_release:
                app.root.current = "rating2"
                root.manager.transition.direction = "left"

        Label:
            id : lb
            text: ''
            color: 0,0,0,1
            font_size:30
            pos_hint: {'x': 0.1 , 'y' : 0.8}
            size_hint: (0.2, 0.1)

        Button:
            text: "Stop"
            font_size: 20
            pos_hint: {'x': 0 , 'y' : 0}
            size_hint: (0.2, 0.1)
            background_color: (1,0,0,1)
            on_press: root.stop()

        Button:
            text: "Start"
            font_size: 20
            pos_hint: {'x': 0 , 'y' : 0.1}
            size_hint: (0.2, 0.1)
            background_color: (0,1,0,1)
            on_press: root.start()

<VRScreen2>:
    name: "rating2"
    vrs2: slide.value

    on_pre_enter:
        slide.value = 0

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height  

        Label:
            text: "Rate the Tremor from 0-10 (minor to severe) \n- nondominant hand"
            font_size: 34

        Label:
            text: str(int(slide.value))
            font_size: 30

        Slider:
            id: slide
            min: 0
            max: 10
            step: 1
            orientation: "horizontal"
           
        Button:
            text: "Next"
            font_size: 20
            size_hint: 0.2, 0.4
            pos_hint: {"right": 1, "bottom": 1}
            background_color: (0,1,1,0.5)
            on_release:
                app.root.current = "results_1"

<ResultScreen1>:
    name: "res1"
    vrs1: 0
    vrs2: 0 

    FloatLayout:
        id: export4
        canvas.before:
            Rectangle:
                pos: self.pos
                size: self.size
                source: "blue.jpeg"

        GridLayout:
            rows:2
            Label: 
                text: "Results" # Part 1
                size_hint: 1,0.05
                font_size: 25
                underline: True
                bold: True
                color: (0,0,0,1)

            #To show visual ratings for dominant and non-dominant hand drawings        
            BoxLayout:
                orientation: "vertical"        
                Label: 
                    text: "Dominant Hand Tremor Visual Rating: {}".format(root.vrs1)
                    font_size: 20
                    color: (0,0,0,1)
                    size_hint: 1, 0.3
                  
                Label: 
                    text: "Non-Dominant Hand Tremor Visual Rating: {}".format(root.vrs2)
                    font_size: 20
                    color: (0,0,0,1)
                    size_hint: 1, 0.2

                #To show tremor index for dominant and non-dominant drawings (1-structural similarity index)   
                GridLayout:
                    cols:2
                    size_hint: 1, 0.3
                    Label: 
                        text: "Tremor Index (dominant hand) = " 
                        font_size: 20
                        color: (0,0,0,1)
                        size_hint: 1, 0.3

                    Label:
                        id: sim_label
                        text: "(no tremor:0 - severe tremor:10)"
                        font_size: 20
                        color: (0,0,0,1)
                        size_hint: 1, 0.3

                GridLayout:
                    cols:2
                    size_hint: 1, 0.2
                    Label: 
                        text: "Tremor Index (non-dominant hand) = " 
                        font_size: 20
                        color: (0,0,0,1)
                        size_hint: 1, 0.3
                        
                    Label:
                        id: sim_label2
                        text: "(no tremor:0 - severe tremor:10)"
                        font_size: 20
                        color: (0,0,0,1)
                        size_hint: 1, 0.3  
                        
                Button:
                    id: ssim_dh
                    text: "Tremor Index" 
                    font_size: 20               
                    size_hint: 0.2, 0.1
                    pos_hint: {"x":0.4, "y": 1}
                    background_color: (0,1,1,0.5)
                    on_press: root.simIndex()
  
                Button:
                    id: save
                    text: "Next"
                    font_size: 20
                    size_hint: 0.3, 0.2
                    pos_hint: {"x":0.7, "y": 0.5}
                    background_color: (0,1,1,1)
                    on_press: root.capture()
                    on_release:
                        app.root.current = "result2"
                        sim_label.text= "(no tremor:0 - severe tremor:10)"   
                        sim_label2.text= "(no tremor:0 - severe tremor:10)"   

<ResultScreen2>:
    name: "result2"
    
    FloatLayout:
        id:export5
        canvas.before:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "blue.jpeg"
        BoxLayout:
            orientation:"vertical"
            padding: [dp(20),0,0,0] 
            
            Label:
                text: "Results Analysis"
                font_size: 25
                bold:True
                background_color:(0,1,1,1)
                color: (0,0,0,1)
                size_hint:0.9,0.05
                pos_hint: {"x":0.05, "y": 0.5}
                valign: "middle"
                padding_top: dp(50)
